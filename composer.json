{
    "name": "jasny/db-gateway",
    "description": "",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Arnold Daniels",
            "email": "arnold@jasny.net",
            "homepage": "http://www.jasny.net"
        }
    ],
    "support": {
        "issues": "https://github.com/jasny/db-gateway/issues",
        "source": "https://github.com/jasny/db-gateway"
    },
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "jasny/php-code-quality": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Jasny\\DBGateway\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jasny\\DBGateway\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpstan analyse",
            "phpunit",
            "phpcs -p src"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
