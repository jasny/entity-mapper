{
    "name": "jasny/persist",
    "description": "ORM/ODM for plain old PHP objects",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Arnold Daniels",
            "email": "arnold@jasny.net",
            "homepage": "http://www.jasny.net"
        }
    ],
    "support": {
        "issues": "https://github.com/jasny/entity-mapper/issues",
        "source": "https://github.com/jasny/entity-mapper"
    },
    "require": {
        "php": ">=7.2.0",
        "improved/improved": "~0.1.0",
        "jasny/php-functions": "dev-master as 4.2.0"
    },
    "require-dev": {
        "jasny/php-code-quality": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Jasny\\Persist\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jasny\\Persist\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpstan analyse",
            "phpunit",
            "phpcs -p src"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
